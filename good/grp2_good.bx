// Recursively compute and print the first 8 fibonacci numbers

proc fib(n : int, m : int, c : int) {
    if (c > 0) {
        var new = m + n : int;
        print(new);
        fib(m, new, c - 1);
    }
}

proc main() {
    print(1);
    print(1);
    fib(1, 1, 6);
}
