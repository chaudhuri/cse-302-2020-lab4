var x = 0, y = 2: int;
var z = 3 : int;

proc main() {
    recurse();
    y = z + 5;
}

proc recurse() {
    var y = true : bool;
    if (x < 100) {
        x = x + 1;
        print(x);
        print(!y);
        recurse();
    } else {
        var y = -0: int;
        print(y*z);
    }
    print(!!y);
}